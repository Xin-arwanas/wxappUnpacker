.type-tag {
    position: absolute;
    right: 0;
}

.type-tag.tag1 {
    top: 0;
}

.type-tag.tag2,.type-tag.tag3 {
    top: 20rpx;
}

._button {
    border-radius: none;
    outline: none;
    margin: 0;
    padding: 0;
    background-color: #fff;
}

._button:after,._button:before {
    border-radius: none;
    outline: none;
    border: none;
}

.global-line {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.global-price {
    position: relative;
    font-size: 48rpx;
    font-weight: 700;
    color: #fc6232;
    line-height: 48rpx;
    margin-left: 4rpx;
}

.global-price:before {
    content: "\FFE5";
    font-size: 30rpx;
    font-weight: 700;
    line-height: 40rpx;
}

.global-button-color {
    background: linear-gradient(180deg,#33271f,#201f1f)!important;
    font-weight: 500!important;
    color: #ecd2a1!important;
}

.global-arrow {
    position: absolute;
    right: 20rpx;
    top: 50%;
    margin-top: -10rpx;
    width: 12rpx;
    height: 20rpx;
    background: url(https://aihuishou-internal.oss-cn-hangzhou.aliyuncs.com/b2c/huangeji/2.0/shangxiang/Shape1%403x.png) no-repeat;
    background-size: 100% 100%;
}

.global-price-color {
    color: #3eb052!important;
}

.global-mask {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background-color: rgba(0,0,0,.6);
    z-index: 3000;
}

@keyframes bounceIn {
    0%,20%,40%,60%,80%,to {
        animation-timing-function: cubic-bezier(.215,.61,.355,1);
    }

    0% {
        opacity: 0;
        transform: scale3d(.3,.3,.3);
    }

    20% {
        transform: scale3d(1.1,1.1,1.1);
    }

    40% {
        transform: scale3d(.9,.9,.9);
    }

    60% {
        opacity: 1;
        transform: scale3d(1.03,1.03,1.03);
    }

    80% {
        transform: scale3d(.97,.97,.97);
    }

    to {
        opacity: 1;
        transform: scaleX(1);
    }
}

@keyframes bounceOut {
    20% {
        transform: scale3d(.9,.9,.9);
    }

    50%,55% {
        opacity: 1;
        transform: scale3d(1.1,1.1,1.1);
    }

    to {
        opacity: 0;
        transform: scale3d(.3,.3,.3);
    }
}

.global-bounceOut {
    animation-name: bounceOut;
}

.global-bounceIn,.global-bounceOut {
    animation-duration: .75s;
}

.global-bounceIn {
    animation-name: bounceIn;
}

._button,._button:after {
    border: none;
    border-radius: 0;
}

.index-box {
    background: #fff;
    box-shadow: 0 0 20rpx 0 hsla(0,0%,84%,.3);
    border-radius: 8rpx;
}

.index-box.index-box-type2 .title {
    text-align: center;
}

.index-box.index-box-type2 .title .arrow,.index-box.index-box-type2 .title:before {
    display: none;
}

.index-box .title {
    position: relative;
    width: auto;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 34rpx;
    font-weight: 600;
    color: #333;
    padding-left: 28rpx;
}

.index-box .title:before {
    content: "";
    position: absolute;
    top: 50%;
    left: 0;
    width: 8rpx;
    height: 32rpx;
    margin-top: -16rpx;
    background: linear-gradient(180deg,#fff03b,#ffd629);
    border-radius: 0 4rpx 4rpx 0;
}

.index-box .title-desc {
    font-size: 26rpx;
    font-weight: 300;
    color: #333;
    line-height: 100rpx;
}

.index-box .title .arrow {
    position: absolute;
    right: 28rpx;
    top: 0;
    width: 100rpx;
    height: 100rpx;
}

.index-box .title .arrow:before {
    content: "";
    position: absolute;
    right: 0;
    top: 50%;
    width: 32rpx;
    height: 32rpx;
    margin-top: -12rpx;
    background: url("https://aihuishou-internal.oss-cn-hangzhou.aliyuncs.com/miniapps/earth/new-index/arrow.png") no-repeat;
    background-size: 100% 100%;
}

.index-box .title-border:after {
    content: "";
    position: absolute;
    left: 32rpx;
    bottom: 0;
    width: 620rpx;
    height: 2rpx;
    background-color: #eee;
}

.index-box .index-box-container {
    padding-left: 32rpx;
    padding-right: 32rpx;
    box-sizing: border-box;
}

.common-model {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background-color: rgba(0,0,0,.8);
    z-index: 101;
}

.common-model .container-box {
    position: absolute;
    left: 50%;
    top: 50%;
    width: 580rpx;
    background: #fff;
    border-radius: 10rpx;
    box-sizing: border-box;
    transform: translate(-50%,-50%);
}

.common-model .title {
    font-size: 36rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    color: #333;
    line-height: 44rpx;
    padding-top: 40rpx;
    margin-bottom: 24rpx;
    text-align: center;
}

.common-model .close {
    position: absolute;
    width: 24rpx;
    height: 24rpx;
    right: 24rpx;
    top: 24rpx;
    background: url("https://sr.aihuishou.com/activity%2Fminapp%2Fhome%2Fclose.png") no-repeat 50%/cover;
}

.common-model .close:before {
    content: "";
    width: 100rpx;
    height: 80rpx;
    position: absolute;
    left: -38rpx;
    top: -38rpx;
}

.common-model .content {
    max-height: 480rpx;
    padding: 0 40rpx 60rpx;
    font-size: 28rpx;
    font-family: PingFangSC-Regular;
    color: #666;
    line-height: 42rpx;
    box-sizing: border-box;
}

.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
}